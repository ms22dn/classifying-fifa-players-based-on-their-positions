    "def multiclass_roc_auc_score(ytest, predictions, average=\"macro\"):\n",
    "    lb = LabelBinarizer()\n",
    "    lb.fit(ytest)\n",
    "    ytest = lb.transform(ytest)\n",
    "    predictions = lb.transform(predictions)\n",
    "    return roc_auc_score(ytest, predictions, average=average)\n",
    "auc = multiclass_roc_auc_score(ytest, predictions, average=\"macro\")\n",
    "print(\"Area under curve : \", auc)"
